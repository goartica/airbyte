          "type": ["null","string"]
        },
        "inputQty": {
          "type": "object",
          "properties" : {
            "unit": {
              "type": "string"
            },
            "amount": {
              "type": "number"
            }
          }
        },
        "availToSellQty": {
          "type": "object",
          "properties" : {
            "unit": {
              "type": "string"
            },
            "amount": {
              "type": "number"
            }
          }
        },
        "reservedQty": {
          "type": "object",
          "properties" : {
            "unit": {
              "type": "string"
            },
            "amount": {
              "type": "number"
            }
          }
        }
      }
    }
  }
}
